<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="integer" hibernate-type="int"></sql-type>
	</type-mapping>
	<table name="GRADE" schema="HIBERNATE" class="entity.Grade">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="STUDENT" schema="HIBERNATE" class="entity.Student">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>